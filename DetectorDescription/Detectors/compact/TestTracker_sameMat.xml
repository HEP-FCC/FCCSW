        <detector id="0" name="BeamTube" type="BeamTube">
            <status id="0"/>
            <dimensions rmin="Tube_rmin" rmax="Tube_rmax" z="Tube_length" vis="comp1" material="Air"/>
        </detector>
        <!--Barrel1-->
